    "class Solution:\n",
    "    def isAnagram(self, s: str, t: str) -> bool:\n",
    "        a=sorted(s)\n",
    "        b=sorted(t)\n",
    "        if a==b:\n",
    "            return True\n",
    "        else :\n",
    "            return False"
